{
  "name": "fazsoft-boilerplate-client",
  "description": "Fazsoft Boilerplate",
  "version": "0.1.0",
  "author": "Fazsoft",
  "homepage": ".",
  "type": "module",
  "scripts": {
    "start": "node index",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev:base": "webpack-dev-server --config webpack.dev.js",
    "dev": "npm run dev:base -- --open",
    "build": "webpack --config webpack.prod.js",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "start-server-and-test dev:base http-get://localhost:8080 test:cypress:run",
    "check": "npm-check -u -s",
    "prepare": ""
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/faker": "^5.1.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^16.9.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/experimental-utils": "^5.62.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-standard": "^5.0.0",
    "favicons-webpack-plugin": "^6.0.1",
    "full-icu": "^1.3.1",
    "git-commit-msg-linter": "^3.2.8",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "jest-localstorage-mock": "^2.4.3",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.3.0",
    "react-inject-env": "^2.1.0",
    "react-router-dom": "^6.22.3",
    "sass-loader": "^12.1.0",
    "start-server-and-test": "^2.0.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.47.5",
    "@fluentui/react-icons": "^2.0.234",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.1",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "main": "index.js",
  "directories": {
    "test": "tests"
  }
}
